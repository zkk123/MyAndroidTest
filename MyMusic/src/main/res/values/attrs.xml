<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="textSize" format="dimension" />
    <attr name="textcolor" format="color" />
    <attr name="weight_value" format="integer">
        <enum name="bottem" value="12"/>
        <enum name="seekbar" value="5"/>
        <enum name="top" value="12"/>
    </attr>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="searchedit">
        <attr name="imagewidth" format="dimension" />
        <attr name="textSize"  />
        <attr name="textcolor"  />
    </declare-styleable>
    <declare-styleable name="MarqueeView">
        <attr name="textSize"  />
        <attr name="textcolor"  />
        <attr name="isRepeat" format="boolean" />
        <attr name="speed" format="integer"/>
        <attr name="startPoint" format="integer">
            <enum name="start" value="0"/>
            <enum name="end" value="1"/>
        </attr>
        <attr name="direction" format="integer">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>
</resources>